{
  "name": "coronavirus-tracker-cli",
  "version": "0.9.1",
  "description": "track conronavirus cases from cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagarkarira/coronavirus-tracker-cli.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/sagarkarira/coronavirus-tracker-cli/issues"
  },
  "homepage": "https://github.com/sagarkarira/coronavirus-tracker-cli#readme",
  "main": "./lib/corona.js",
  "bin": {
    "corona": "bin/index.js"
  },
  "scripts": {
    "dev": "nodemon --inspect app.js",
    "lint": "xo",
    "start": "node app.js",
    "test": "npm run lint"
  },
  "files": [
    "bin/*.js",
    "lib/*.js",
    "app.js"
  ],
  "engines": {
    "node": ">=12.x"
  },
  "author": "sagarkarira1992@gmail.com",
  "license": "ISC",
  "dependencies": {
    "asciichart": "^1.5.11",
    "axios": "^0.19.2",
    "chalk": "^3.0.0",
    "cli-table3": "^0.5.1",
    "country-code-lookup": "0.0.17",
    "emoji-flags": "^1.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "humanize-number": "0.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.0",
    "strip-ansi": "^6.0.0",
    "yargonaut": "^1.1.4",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "xo": "^0.28.2"
  },
  "xo": {
    "space": 2,
    "rules": {
      "arrow-parens": "off",
      "capitalized-comments": "off",
      "no-negated-condition": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "promise/prefer-await-to-then": "off",
      "quote-props": "off",
      "space-before-function-paren": "off",
      "unicorn/filename-case": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/string-content": "off"
    }
  }
}
